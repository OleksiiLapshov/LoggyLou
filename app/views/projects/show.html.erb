<div class="form-card">
  <h1  class="h1-style">Project card</h1>
  <%= render @project %>
  <div class="flex pt-6 gap-x-2">
    <%= link_to "Edit", edit_project_path(@project), class: "button" %>
    <%= back_button %>
    <%= button_to "Delete project", @project, method: :delete, class: "button-danger" %>
</div>
</div>



