<div id="<%= dom_id project %>">
  <p>
    <strong>Name:</strong>
    <%= project.name %>
  </p>

  <p>
    <strong>Company:</strong>
    <%= link_to "#{project.company.name}", project.company, class: "underline" %>
  </p>
  <p>
    <strong>Assigned to:</strong>
    <% project.users.each do |user| %>
      <p class="underline"><%= link_to "#{user.first_name} #{user.last_name}", user %></p>
    <% end %>
  </p>

</div>
