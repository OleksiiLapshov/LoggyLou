<div id="<%= dom_id worklog %>">
  <p>
    <strong>Employee:</strong>
    <%= worklog.employee %>
  </p>

  <p>
    <strong>Hours:</strong>
    <%= worklog.hours %>
  </p>

  <p>
    <strong>Note:</strong>
    <%= worklog.note %>
  </p>

  <p>
    <strong>Project:</strong>
    <%= worklog.project.name %>
  </p>

  <p>
    <strong>Log date:</strong>
    <%= worklog.log_date %>
  </p>

</div>
