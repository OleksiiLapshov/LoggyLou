<p style="color: green"><%= notice %></p>

<% content_for :title, "Worklogs" %>

<h1>Worklogs</h1>

<div id="worklogs">
  <% @worklogs.each do |worklog| %>
    <%= render worklog %>
    <p>
      <%= link_to "Show this worklog", worklog, class: "button-secondary" %>
    </p>
  <% end %>
</div>

<%= link_to "New worklog", new_worklog_path, class: "button" %>
